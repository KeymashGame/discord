{
  "name": "discord",
  "version": "1.0.0",
  "description": "Official Discord bot for Keymash",
  "type": "module",
  "engines": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  },
  "engineStrict": true,
  "scripts": {
    "build": "tsc",
    "run": "node --experimental-specifier-resolution=node src --enable-source-maps",
    "start": "yarn build && yarn run run",
    "dev": "nodemon",
    "pretty": "prettier 'src/**/*.ts'",
    "pretty-fix": "prettier --write 'src/**/*.ts'",
    "lint": "eslint -c .eslintrc.json 'src/**/*.ts'",
    "lint-fix": "eslint --fix -c .eslintrc.json 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keyma-sh/discord.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keyma-sh/discord/issues"
  },
  "homepage": "https://github.com/keyma-sh/discord#readme",
  "dependencies": {
    "colors": "1.4.0",
    "discord.js": "14.11.0",
    "dotenv": "16.0.3",
    "glob": "10.2.2",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "4.14.194",
    "@types/node": "20.1.0",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "yarn": "1.22.19"
  }
}
