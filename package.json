    "colors": "1.4.0",
    "discord.js": "13.7.0",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "eslint": "8.16.0",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "ts-node": "10.8.0",
    "typescript": "4.7.2",
